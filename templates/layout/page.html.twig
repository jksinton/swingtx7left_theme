<!-- Header and Navbar -->
<header class="main-header">
  <!-- Top Navbar -->
  <nav class="navbar topnav navbar-default" role="navigation">
	<div class="container">
	  <div class="row">
	  <div class="navbar-header col-md-3">
		<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-navigation">
		  <span class="sr-only">Toggle navigation</span>
		  <span class="icon-bar"></span>
		  <span class="icon-bar"></span>
		  <span class="icon-bar"></span>
		</button>
		{% if page.header %}
		  {{ page.header }}
		{% endif %}
	  </div>
	  <!-- Navigation -->
	  <div class="col-md-9">
		{% if page.primary_menu %}
		  {{ page.primary_menu }}
		{% endif %}      
	  </div>
	  <!--End Navigation -->
	  </div>
	</div>
  </nav>
  <!--End Navbar -->
</header>
<!--End Header & Navbar -->

{% if is_front %}
	<!-- intro  scroll magic  -->
    <section class="intro-scroll" id="section-wipes">
		<div id="pinContainer">
			<section class="intro-panel texas-blue-1">
			<div class="left-bar">
			  <div class="star-left">
				<img src="{{ base_path }}themes/swingtx7left_theme/images/texas-star-vertical.png">
			  </div>
			</div>  
			<div class="caption"><div class="border"><b>It's time.</b></div></div>
			</section>
			<section class="intro-panel texas-red">
			<div class="caption"><div class="border"><b>To bring progress to TX-07.</b></div></div>
				</section>
			<section class="intro-panel texas-white">
				<div class="caption"><div class="border"><b>We have the ability.</b></div></div>
			</section>
			<section class="intro-panel texas-blue-2">
			<div class="right-bar">
			  <div class="star-right">
				<img src="{{ base_path }}themes/swingtx7left_theme/images/texas-star-vertical.png">
			  </div>
			</div>  
				<div class="caption"><div class="border"><b>You bring the momentum.</b></div></div>
			</section>
		  <section class="intro-panel texas-flag">
			<div class="texas-flag-bg"></div>
			<div class="caption"><div id="joinus-pin" class="border"><b>Join us.</b></div></div>
		  </section>
 		</div>
	</section>
	<!-- End intro  scroll magic  -->
    <section id="next-section">
{% endif %}

<!--Search-->
  {% if is_front and page.search %}
    {{ page.search }}
  {% endif %}
<!--End Search-->

<!--Help-->
  {% if page.help %}
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          {{ page.help }}
        </div>
      </div>
    </div>
  {% endif %}
<!--End Help-->

<!-- Page Title -->
{%  if page.page_title and not is_front %}
  <div id="page-title">
    <div id="page-title-inner">
      <!-- start: Container -->
      <div class="container">
        {{ page.page_title }}
      </div>
    </div>
  </div>
{% endif %}
<!-- End Page Title -->

<!-- layout -->
  <div id="wrapper">
	  <!-- start: Container -->
	  <div class="container">
		  <!--- Start content -->
		  {% if page.content %}
			<div class="content_layout">
			  <div class={{contentlayout}}>
				{{ page.content }}
			  </div>
			</div>
		  {% endif %}
		  <!---End content -->
	  </div>
	  <!-- End container -->
	</div>
<!-- End layout -->
  

<!-- Middle  parallax background -->
{% if is_front %}  
  <div class=" parallax-2">
  </div>
{% endif %}
<!-- End Middle  parallax background -->

<!-- Grid Panel One -->
{% if is_front %}  
      {% if page.grid_panel_one %}
      	<div class="container">
			{{ page.grid_panel_one }}
		</div>

	    <!-- Bottom  parallax background -->
	    {% if is_front %}  
		  <div class="parallax-2">
		  </div>
	    {% endif %}
	    <!-- End Bottom  parallax background -->
	  {% endif %}
{% endif %}
<!-- End  Grid Panel One-->

<!-- Start Footer Ribbon -->
{% if page.footer_menu %}
  <div class="footer-ribbon">
    <div class="container">
      <div class="row">
        {{ page.footer_menu }}
      </div>
    </div>
  </div>
{% endif %}
<!-- End Footer Ribbon -->

<!-- Start Social Ribbon -->
{% if is_front %}  
  <div class="social-ribbon front">
{% else %}  
  <div class="social-ribbon">
{% endif %}  
    <div class="container">
      <div class="row">
        {% if show_social_icon %}
			{% if is_front %}  
				<div class="social-media front">
    		{% else %}  
  				<div class="social-media">
			{% endif %}  
        	{% if facebook_url %}
              		<a href="{{ facebook_url }}" class="icon-facebook" data-toggle="tooltip" data-placement="bottom" title="Facebook"><i class="fa fa-facebook fa-4"></i></a>
            	{% endif %}
				{% if google_plus_url %}
              		<a href="{{ google_plus_url }}" class="icon-gplus" data-toggle="tooltip" data-placement="bottom" title="Google+"><i class="fa fa-google-plus"></i></a>
            	{% endif %}
            	{% if twitter_url %}
              		<a href="{{ twitter_url }}" class="icon-twitter" data-toggle="tooltip" data-placement="bottom" title="Twitter"><i class="fa fa-twitter"></i></a>
				{% endif %}
				{% if linkedin_url %}
					<a href="{{ linkedin_url }}" class="icon-linkedin" data-toggle="tooltip" data-placement="bottom" title="LinkedIn"><i class="fa fa-linkedin"></i></a>
				{% endif %}
				{% if ytube_url %}
					<a href="{{ ytube_url }}" class="icon-youtube" data-toggle="tooltip" data-placement="bottom" title="YouTube"><i class="fa fa-youtube-play"></i></a>
				{% endif %}
        	</div>
        {% endif %}
      </div>
    </div>
  </div>
<!-- End Social Ribbon -->

<!-- #footer-bottom -->
<div id="footer-bottom">
    <div class="container">
         <div class="row">
		 <hr class="style-mint">
         <div id="toTop" class="col-md-12">
          <a class="scrollUp"><i class="fa fa-angle-up"></i></a>
         </div>
        </div>
        <div class="row">
        <div class="col-md-12 copy_credit">
        <!-- <p class="copyright">Copyright &copy; {{ "now"|date("Y") }}. All rights reserved.</p> -->
        <p class="credit"><a href="/privacy-policy" target="_blank">Privacy Policy</a></p>
        <p class="credit"><a href="/disclaimer" target="_blank">Disclaimer</a></p>
	
        {% if show_credit_link %}
          <p class="credit">Theme for <a href="https://www.drupal.org/8" target="_blank">Drupal 8</a></p>
        {% endif %}
        </div>
		</div>
    </div>
</div>
<!-- #footer-bottom ends here -->

{% if is_front %}
  </section>
  <!-- js libraries for fancy scrollmagic -->
  {{ attach_library('swingtx7left_theme/intro-scroll') }}
{% endif %}
